@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    @Html.DropDownList("custData",(List<SelectListItem>)ViewBag.custData)
    <div>
        @{
            foreach (var item in (List<eSalex.Models.Order>)ViewBag.order)
            {
                @item.CustId
                @item.CustName
                @item.EmpId
                @item.EmpName
                @item.OrderDate<br>
            }
        }
    </div>
</body>
</html>
