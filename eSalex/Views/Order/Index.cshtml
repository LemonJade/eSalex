@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        @{
            foreach (var item in ViewBag.order)
            {
                @item.CustId
                @item.CustName
                @item.EmpId
                @item.EmpName
                @item.OrderDate<br>
            }
        }
    </div>
</body>
</html>
